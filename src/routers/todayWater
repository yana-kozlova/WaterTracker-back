import { Router } from 'express';
import { authenticate } from '../middlewares/authenticate.js';
import validateBody from '../utils/validateBody.js';


const todayWaterRouter = Router();


todayWaterRouter.use(authenticate);

todayWaterRouter.post('/add', ctrlWrapper());
todayWaterRouter.delete('/:id', validateBody(), ctrlWrapper());
todayWaterRouter.patch('/edit/:id', validateBody(), ctrlWrapper());


export default todayWaterRouter;

